generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       String  @id @default(uuid())
  name     String
  email    String  @unique
  password String
  perfil   String?
}
